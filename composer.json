{
    "name": "affinidi-tdk/affinidi-tdk-php",
    "description": "Affinidi Trust Developement Kit",
    "type": "library",
    "license": "Apache-2.0",
    "keywords": [
        "trust"
    ],
    "autoload": {
        "psr-4": {
            "AffinidiTdk\\": "src/",
            "AffinidiTdk\\Clients\\CredentialIssuance\\": "src/Clients/CredentialIssuanceClient/lib/",
            "AffinidiTdk\\Clients\\CredentialVerification\\": "src/Clients/CredentialVerificationClient/lib/",
            "AffinidiTdk\\Clients\\Iam\\": "src/Clients/IamClient/lib/",
            "AffinidiTdk\\Clients\\Iota\\": "src/Clients/IotaClient/lib/",
            "AffinidiTdk\\Clients\\LoginConfiguration\\": "src/Clients/LoginConfigurationClient/lib/",
            "AffinidiTdk\\Clients\\Wallets\\": "src/Clients/WalletsClient/lib/"
        }
    },
    "authors": [
        {
            "name": "Affinidi",
            "email": "info@affinidi.com",
            "homepage": "https://affinidi.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0",
        "firebase/php-jwt": "^6.0",
        "ramsey/uuid": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "minimum-stability": "stable"
}
